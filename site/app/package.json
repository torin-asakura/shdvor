    "@sentry/nextjs": "8.24.0",
    "@sentry/utils": "8.24.0",
    "@site/contacts-page": "workspace:*",
    "@site/index-page": "workspace:*",
    "@site/root-layout-fragment": "workspace:*",
    "@site/service-page": "workspace:*",
    "@site/services-page": "workspace:*",
    "@types/events": "^3.0.3",
    "@types/express": "^4.17.21",
    "@types/node": "22.1.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/recompose": "^0.30.15",
    "@types/styled-system": "^5.1.22",
    "@ui/progress-bar": "workspace:*",
